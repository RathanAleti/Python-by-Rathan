# Using turn_right function to make the robot turn right 
def turn_right():
    turn_left()
    turn_left()
    turn_left()
   
# Using While loop to make robot home   
while at_goal()!= True:
    if right_is_clear():
        turn_right()
        if front_is_clear():
            move()
    elif front_is_clear():
            move()
    else:
        turn_left()
